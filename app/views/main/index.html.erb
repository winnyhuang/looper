<div id="container">Loading..</div> <!--where the events will populate-->

<script>
	for (i=1; i<10; i++){
		var klass = "." + i
		$(klass).on('click', this, function(event){
				var myClass = $(this).attr("class");
				var id = "#" + myClass
				alert(id);
				$("html, body").animate({ scrollTop: $(id).offset().top }, 1000);
		});
	}
</script>	

